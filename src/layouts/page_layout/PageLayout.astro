---
import Head from "../html/Head.astro";
import { NavBar } from "@components/blocks";
import { Logo } from "@components/lib_comps";

import Footer from "../html/Footer.astro";

const { pageId } = Astro.props;
---

<html lang="en">
  <Head />
  <body id={pageId}>
    <header>
      <NavBar />
      <Logo />
    </header>

    <slot />
    <Footer />
  </body>
</html>
