---
import Head from "./Head.astro";
import Body from "./Body.astro";
import Footer from "./Footer.astro";

const { pageId } = Astro.props;
---

<html lang="en">
  <Head />
  <Body pageId={pageId}>
    <slot />
    <Footer />
  </Body>
  <style>
    body {
      display: flex;
      flex-direction: column;
      justify-content: center;
      max-width: 1024px;
      margin: auto;
    }
  </style>


</html>
