---
    import JobPosition from "./JobPosition.astro"
    const {positionTitle} = Astro.props
---
    
<ul>
    {positionTitle.map((job)=> (<JobPosition job={job} />))}
</ul>

<style>
    ul{
        padding: 0px;
        margin: 0;
        display: flex;
        flex-direction: column;
    }
</style>